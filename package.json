{
  "name": "@charlietango/babel-preset-sx-prop",
  "version": "1.1.1",
  "description": "Preset to consume the sx prop on all elements",
  "main": "index.js",
  "author": "Charlie Tango",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/charlie-tango/babel-preset-sx-prop.git"
  },
  "private": false,
  "keywords": [
    "emotion",
    "styled-system",
    "ui"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@charlietango/emotion-sx": "^1.0.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "babel-plugin-emotion": "^10.0.27"
  },
  "peerDependencies": {
    "babel-core": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^9.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
